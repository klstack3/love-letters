{
  "version": 2,
  "functions": {
    "server/index.ts": {
      "runtime": "@vercel/node@22.10.5"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.ts"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "./client/dist",
  "installCommand": "npm install",
  "env": {
    "NODE_ENV": "production"
  }
}
